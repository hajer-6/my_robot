<launch>
    <!-- Serial node for /dev/ttyACM0 -->
    <node pkg="rosserial_python" type="serial_node.py" name="serial_acm0" output="screen">
        <param name="port" value="/dev/ttyACM0"/>
        <param name="baud" value="57600"/>
    </node>

    <!-- Serial node for /dev/ttyACM1 -->
    <node pkg="rosserial_python" type="serial_node.py" name="serial_acm1" output="screen">
        <param name="port" value="/dev/ttyACM1"/>
        <param name="baud" value="57600"/>
    </node>

    <!-- Kinematics node -->
    <node pkg="kinematics" type="kinematics.py" name="kinematics" output="screen"/>

    <!-- bno node -->
    <node pkg="imu_message" type="pubYaw.py" name="bno" output="screen"/>
</launch>
